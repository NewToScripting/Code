<Ribbon:RibbonWindow x:Class="Inspire.Client.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="INSPIRE Designer"
    xmlns:ConfigurationWindow="clr-namespace:Inspire.Client.ConfigurationWindow;assembly=Inspire.Client.ConfigurationWindow" 
    xmlns:ScheduleWindow="clr-namespace:Inspire.Client.ScheduleWindow;assembly=Inspire.Client.ScheduleWindow" 
    xmlns:Designer="clr-namespace:Inspire.Client.Designer;assembly=Inspire.Client.Designer" 
    xmlns:Commands="clr-namespace:Inspire.Commands;assembly=Inspire"
    xmlns:Actipro="clr-namespace:ActiproSoftware.Windows.Controls.Ribbon.Controls;assembly=ActiproSoftware.Ribbon.WPF30"
    xmlns:Input="clr-namespace:ActiproSoftware.Windows.Controls.Ribbon.Input;assembly=ActiproSoftware.Ribbon.WPF30"
    xmlns:media="clr-namespace:System.Windows.Media;assembly=PresentationCore"
    xmlns:UI="clr-namespace:ActiproSoftware.Windows.Controls.Ribbon.UI;assembly=ActiproSoftware.Ribbon.WPF30"
    xmlns:Inspire="clr-namespace:Inspire;assembly=Inspire"
    xmlns:Ribbon="clr-namespace:DevComponents.WpfRibbon;assembly=DevComponents.WpfRibbon"
    xmlns:DevEditors="clr-namespace:DevComponents.WpfEditors;assembly=DevComponents.WpfEditors"
    xmlns:Dock="clr-namespace:DevComponents.WpfDock;assembly=DevComponents.WpfDock"
    xmlns:Client="clr-namespace:Inspire.Client"
    xmlns:ScheduleTreeView="clr-namespace:Inspire.Client.ScheduleWindow.ScheduleTreeView;assembly=Inspire.Client.ScheduleWindow"
    xmlns:AdminTreeView="clr-namespace:Inspire.Client.ConfigurationWindow.AdminTreeView;assembly=Inspire.Client.ConfigurationWindow"
    xmlns:DesignPropertyPanel1="clr-namespace:Inspire.Client.Designer.DesignPropertyPanel;assembly=Inspire.Client.Designer"
    ResizeMode="CanResize" WindowStartupLocation="CenterScreen" x:Name="InspireUI"
    MinWidth="850" 
    MinHeight="730"
    FontSize="7.75pt"
    Foreground="#DDD"
    FontFamily="Verdana">

    <Ribbon:RibbonWindow.Resources>
        <Style x:Key="AppMenuCommandButton" TargetType="{x:Type Ribbon:ButtonDropDown}">
            <Setter Property="Padding" Value ="4,2,4,2"/>
        </Style>
        <Style x:Key="AppMenuSeparator" TargetType="{x:Type Separator}">
            <Setter Property="Margin" Value ="40,0,0,0"/>
        </Style>
    </Ribbon:RibbonWindow.Resources>

    <Ribbon:RibbonWindow.CommandBindings>
        <CommandBinding Command="{x:Static Commands:InspireCommands.NewSlideCommand}" Executed="NewSlideExecuted" CanExecute="NewSlideCanExecute" />
        <CommandBinding Command="{x:Static Commands:InspireCommands.SchedulerNewSlideCommand}" Executed="SchedulerNewSlideExecuted" CanExecute="SchedulerNewSlideCanExecute" />
        <CommandBinding Command="{x:Static Commands:InspireCommands.SaveSlideCommand}" Executed="SaveSlideExecuted" CanExecute="SaveSlideCanExecute" />
        <CommandBinding Command="{x:Static Commands:InspireCommands.SaveSlideToServerCommand}" Executed="SaveSlideToServerExecuted" CanExecute="SaveSlideToServerCanExecute" />
        <CommandBinding Command="{x:Static Commands:InspireCommands.SaveSlideLocalCommand}" Executed="SaveSlideLocalExecuted" CanExecute="SaveSlideLocalCanExecute" />
        <CommandBinding Command="{x:Static Commands:InspireCommands.ExportSlideCommand}" Executed="ExportSlideExecuted" CanExecute="ExportSlideCanExecute" />
        <CommandBinding Command="{x:Static Commands:InspireCommands.LoadSlideCommand}" Executed="LoadSlideExecuted" CanExecute="LoadSlideCanExecute" />
        <CommandBinding Command="{x:Static Commands:InspireCommands.EditSlideCommand}" Executed="EditSlideExecuted" CanExecute="EditSlideCanExecute" />
        <CommandBinding Command="{x:Static Commands:InspireCommands.DeleteSlideCommand}" Executed="DeleteSlideExecuted" CanExecute="DeleteSlideCanExecute" />
        <CommandBinding Command="{x:Static Commands:InspireCommands.ClearSlideCommand}" Executed="ClearSlideExecuted" CanExecute="ClearSlideCanExecute" />
        <CommandBinding Command="{x:Static Commands:InspireCommands.ShowDesignerCommand}" Executed="ShowDesignerExecuted" CanExecute="ShowDesignerCanExecute" />
        <CommandBinding Command="{x:Static Commands:InspireCommands.ShowSchedulerCommand}" Executed="ShowSchedulerExecuted" CanExecute="ShowSchedulerCanExecute" />
        <CommandBinding Command="{x:Static Commands:InspireCommands.ShowConfigurationCommand}" Executed="ShowConfigurationExecuted" CanExecute="ShowConfigurationCanExecute" />
        <CommandBinding Command="{x:Static Commands:InspireCommands.UndoCommand}" Executed="UndoCommandExecuted" CanExecute="UndoCommandCanExecute" />
        <CommandBinding Command="{x:Static Commands:InspireCommands.RedoCommand}" Executed="RedoCommandExecuted" CanExecute="RedoCommandCanExecute" />
        <CommandBinding Command="{x:Static Commands:InspireCommands.DeleteCommand}" Executed="DeleteCommandExecuted" CanExecute="DeleteCommandCanExecute" />
        <CommandBinding Command="{x:Static Commands:InspireCommands.CopyCommand}" Executed="CopyCommandExecuted" CanExecute="CopyCommandCanExecute" />
        <CommandBinding Command="{x:Static Commands:InspireCommands.DuplicateCommand}" Executed="DuplicateCommandExecuted" CanExecute="DuplicateCommandCanExecute" />
        <CommandBinding Command="{x:Static Commands:InspireCommands.PasteCommand}" Executed="PasteCommandExecuted" CanExecute="PasteCommandCanExecute" />
        <CommandBinding Command="{x:Static Commands:InspireCommands.CutCommand}" Executed="CutCommandExecuted" CanExecute="CutCommandCanExecute" />
        <CommandBinding Command="{x:Static Commands:InspireCommands.UpdateDisplaysCommand}" Executed="UpdateDisplaysCommandExecuted" CanExecute="UpdateDisplaysCommandCanExecute" />
        <CommandBinding Command="{x:Static Commands:InspireCommands.AlignLeftCommand}" Executed="AlignLeftCommandExecuted" CanExecute="AlignLeftCommandCanExecute" />
        <CommandBinding Command="{x:Static Commands:InspireCommands.AlignHorizontalCentersCommand}" Executed="AlignHorizontalCentersCommandExecuted" CanExecute="AlignHorizontalCentersCommandCanExecute" />
        <CommandBinding Command="{x:Static Commands:InspireCommands.AlignRightCommand}" Executed="AlignRightCommandExecuted" CanExecute="AlignRightCommandCanExecute" />
        <CommandBinding Command="{x:Static Commands:InspireCommands.AlignTopCommand}" Executed="AlignTopCommandExecuted" CanExecute="AlignTopCommandCanExecute" />
        <CommandBinding Command="{x:Static Commands:InspireCommands.AlignVerticalCentersCommand}" Executed="AlignVerticalCentersCommandExecuted" CanExecute="AlignVerticalCentersCommandCanExecute" />
        <CommandBinding Command="{x:Static Commands:InspireCommands.AlignBottomCommand}" Executed="AlignBottomCommandExecuted" CanExecute="AlignBottomCommandCanExecute" />
        <CommandBinding Command="{x:Static Commands:InspireCommands.DistributeHorizontalCommand}" Executed="DistributeHorizontalCommandExecuted" CanExecute="DistributeHorizontalCommandCanExecute" />
        <CommandBinding Command="{x:Static Commands:InspireCommands.DistributeVerticalCommand}" Executed="DistributeVerticalCommandExecuted" CanExecute="DistributeVerticalCommandCanExecute" />
        <CommandBinding Command="{x:Static Commands:InspireCommands.PlayCanvasCommand}" Executed="PlayCanvasCommandExecuted" CanExecute="PlayCanvasCommandCanExecute" />
        <CommandBinding Command="{x:Static Commands:InspireCommands.StopCanvasCommand}" Executed="StopCanvasCommandExecuted" CanExecute="StopCanvasCommandCanExecute" />
        <CommandBinding Command="{x:Static Commands:InspireCommands.PlayScheduleCommand}" Executed="PlayScheduleCommandExecuted" CanExecute="PlayScheduleCommandCanExecute" />
        <CommandBinding Command="{x:Static Commands:InspireCommands.ShowLayersPanel}" Executed="ShowLayersPanelCommandExecuted" CanExecute="ShowLayersPanelCommandCanExecute" />
        <CommandBinding Command="{x:Static Commands:InspireCommands.Rotate90RightCommand}" Executed="Rotate90RightCommandExecuted" CanExecute="Rotate90RightCommandCanExecute" />
        <CommandBinding Command="{x:Static Commands:InspireCommands.Rotate90LeftCommand}" Executed="Rotate90LeftCommandExecuted" CanExecute="Rotate90LeftCommandCanExecute" />
        <CommandBinding Command="{x:Static Commands:InspireCommands.FlipVerticalCommand}" Executed="FlipVerticalCommandExecuted" CanExecute="FlipVerticalCommandCanExecute" />
        <CommandBinding Command="{x:Static Commands:InspireCommands.FlipHorizontalCommand}" Executed="FlipHorizontalCommandExecuted" CanExecute="FlipHorizontalCommandCanExecute" />
        <CommandBinding Command="{x:Static Commands:InspireCommands.LoadOfflineFileDialogCommand}" Executed="LoadOfflineFileDialogCommandExecuted" CanExecute="LoadOfflineFileDialogCanExecute" />
    </Ribbon:RibbonWindow.CommandBindings>
    <Grid>
        <TextBlock Foreground="Black" VerticalAlignment="Top" HorizontalAlignment="Right" Height="20" Width="200" Margin="0,5,120,0" FontSize="14" TextAlignment="Right" Text="{Binding Inspire:User.UserName}">
            <TextBlock.Effect>
                <DropShadowEffect Color="White" BlurRadius="5" Direction="-446" ShadowDepth="-2"/>
            </TextBlock.Effect>
        </TextBlock>
        <DockPanel LastChildFill="True" VerticalAlignment="Top">
            <Ribbon:Ribbon x:Name="mainApplictionRibbon" LicenseKey="DAD80300DA22"  DockPanel.Dock="Top" ><!---->
                <Ribbon:Ribbon.QuickAccessToolbar>
                    <Ribbon:Qat x:Name="quickAT">
                        <Ribbon:ButtonDropDown PartVisibility="ImageOnly" Ribbon:Ribbon.KeyTip="S" ToolTip="Save Slide" Command="{x:Static Commands:InspireCommands.SaveSlideCommand}">
                            <Ribbon:ButtonDropDown.Image>
                                <Image Width="16" Height="16" Source="Resources/Images/save16.png" />
                            </Ribbon:ButtonDropDown.Image>
                        </Ribbon:ButtonDropDown>
                        <Ribbon:ButtonDropDown PartVisibility="ImageOnly" Ribbon:Ribbon.KeyTip="Z" ToolTip="Undo" Command="{x:Static Commands:InspireCommands.UndoCommand}">
                            <Ribbon:ButtonDropDown.Image>
                                <Image Width="16" Height="16" Source="Resources/Images/undo16.png" />
                            </Ribbon:ButtonDropDown.Image>
                        </Ribbon:ButtonDropDown>
                        <Ribbon:ButtonDropDown PartVisibility="ImageOnly" Ribbon:Ribbon.KeyTip="Y" ToolTip="Redo" Command="{x:Static Commands:InspireCommands.RedoCommand}">
                            <Ribbon:ButtonDropDown.Image>
                                <Image Width="16" Height="16" Source="Resources/Images/redo16.png" />
                            </Ribbon:ButtonDropDown.Image>
                        </Ribbon:ButtonDropDown>
                        <Ribbon:ButtonDropDown PartVisibility="ImageOnly" Ribbon:Ribbon.KeyTip="N" ToolTip="New Slide" Command="{x:Static Commands:InspireCommands.NewSlideCommand}">
                            <Ribbon:ButtonDropDown.Image>
                                <Image Width="16" Height="16" Source="Resources/Images/New16.png" />
                            </Ribbon:ButtonDropDown.Image>
                        </Ribbon:ButtonDropDown>
                    </Ribbon:Qat>
                </Ribbon:Ribbon.QuickAccessToolbar>
                <Ribbon:Ribbon.ApplicationMenu>
                    <Ribbon:ApplicationMenu x:Name="appMenu">
                        <Ribbon:ApplicationMenu.Image>
                            <Image Source="AllIcons.ico" Width="24" Height="24"/>
                        </Ribbon:ApplicationMenu.Image>
                        <Ribbon:ApplicationMenu.AppItems>
                            <Ribbon:ButtonDropDown Foreground="#DDD" Header="Exit Application" />
                        </Ribbon:ApplicationMenu.AppItems>
                        <Ribbon:ButtonDropDown Command="{x:Static Commands:InspireCommands.LoadOfflineFileDialogCommand}">
                            <Ribbon:ButtonDropDown.Image>
                                <Image Source="Resources/Images/New32.png" Height="32" Width="32" Margin="0,0,10,0"/>
                            </Ribbon:ButtonDropDown.Image>
                            <Ribbon:ButtonDropDown.Header>Import Slide / Template</Ribbon:ButtonDropDown.Header>

                        </Ribbon:ButtonDropDown>
                        <Separator />
                        <Ribbon:ButtonDropDown>
                            <Ribbon:ButtonDropDown.Image>
                                <Image Source="Resources/Images/Update.png" Height="32" Width="32" Margin="0,0,10,0"/>
                            </Ribbon:ButtonDropDown.Image>
                            <Ribbon:ButtonDropDown.Header>Force Update for All Displays</Ribbon:ButtonDropDown.Header>
                        </Ribbon:ButtonDropDown>
                    </Ribbon:ApplicationMenu>
                </Ribbon:Ribbon.ApplicationMenu>
                <!--Scheduling Home-->
                <Ribbon:RibbonTab Header="Home" x:Name="scheduleTab" Visibility="Collapsed" FontSize="10" Ribbon:Ribbon.KeyTip="S">
                    <Ribbon:RibbonBarPanel Height="90" KeyboardNavigation.DirectionalNavigation="Cycle" KeyboardNavigation.TabNavigation="Cycle">
                        <Ribbon:RibbonBar Header="Slide" Padding="3" Ribbon:Ribbon.KeyTip="FO">
                            <Ribbon:ButtonPanel Orientation="Horizontal" VerticalAlignment="Stretch">
                                <Ribbon:ButtonDropDown ImagePosition="Top" Ribbon:RibbonBar.MinAutoSizeHint="Medium" ExpandPosition="Bottom" InlineExpand="False" PopupType="Menu" ContentExpands="False" VerticalContentAlignment="Center" Ribbon:Ribbon.KeyTip="N" Command="{x:Static Commands:InspireCommands.SchedulerNewSlideCommand}">
                                    <Ribbon:ButtonDropDown.Image>
                                        <Image Source="Resources/Images/New32.png" Stretch="None"/>
                                    </Ribbon:ButtonDropDown.Image>
                                    <Ribbon:ButtonDropDown.Header>New</Ribbon:ButtonDropDown.Header>
                                </Ribbon:ButtonDropDown>
                            </Ribbon:ButtonPanel>
                        </Ribbon:RibbonBar>
                        <Ribbon:RibbonBar Header="Schedule" Padding="3" Ribbon:Ribbon.KeyTip="FO">
                            <Ribbon:ButtonPanel Orientation="Horizontal" VerticalAlignment="Stretch">
                                <Ribbon:ButtonDropDown ImagePosition="Top" Ribbon:RibbonBar.MinAutoSizeHint="Medium" ExpandPosition="Bottom" InlineExpand="False" PopupType="Menu" ContentExpands="False" VerticalContentAlignment="Center" Ribbon:Ribbon.KeyTip="N" Command="{x:Static Commands:InspireCommands.NewScheduleCommand}">
                                    <Ribbon:ButtonDropDown.Image>
                                        <Image Source="Resources/Images/Schedule.png" Height="35"  Stretch="Fill"/>
                                    </Ribbon:ButtonDropDown.Image>
                                    <Ribbon:ButtonDropDown.Header>New</Ribbon:ButtonDropDown.Header>
                                </Ribbon:ButtonDropDown>
                                <Ribbon:ButtonDropDown ImagePosition="Top" Ribbon:RibbonBar.MinAutoSizeHint="Medium" ExpandPosition="Bottom" InlineExpand="False" PopupType="Menu" ContentExpands="False" VerticalContentAlignment="Center" Ribbon:Ribbon.KeyTip="D" Command="{x:Static Commands:InspireCommands.DeleteScheduleCommand}" >
                                    <Ribbon:ButtonDropDown.Image>
                                        <Image Source="Resources/Images/Remove.png" Stretch="Fill" SnapsToDevicePixels="True" Height="16" Width="16" Margin="3"/>
                                    </Ribbon:ButtonDropDown.Image>
                                    <Ribbon:ButtonDropDown.Header>Delete</Ribbon:ButtonDropDown.Header>
                                </Ribbon:ButtonDropDown>
                            </Ribbon:ButtonPanel>
                        </Ribbon:RibbonBar>
                        <Ribbon:RibbonBar Header="Update" Padding="3" Dock:DockSite.Dock="Right" Ribbon:Ribbon.KeyTip="FO">
                            <Ribbon:ButtonPanel Orientation="Horizontal" VerticalAlignment="Stretch" Margin="7,0">
                                <Ribbon:ButtonDropDown ImagePosition="Top" Ribbon:RibbonBar.MinAutoSizeHint="Medium" ExpandPosition="Bottom" InlineExpand="False" PopupType="Menu" ContentExpands="False" VerticalContentAlignment="Center" Ribbon:Ribbon.KeyTip="U" Command="{x:Static Commands:InspireCommands.UpdateDisplaysCommand}">
                                    <Ribbon:ButtonDropDown.Image>
                                        <Image Source="Resources/Images/Update.png" Height="32"/>
                                    </Ribbon:ButtonDropDown.Image>
                                    <Ribbon:ButtonDropDown.Header>Update Displays</Ribbon:ButtonDropDown.Header>
                                </Ribbon:ButtonDropDown>
                            </Ribbon:ButtonPanel>
                        </Ribbon:RibbonBar>
                        <Ribbon:RibbonBar Header="Preview" Padding="3" Ribbon:Ribbon.KeyTip="FO" HorizontalContentAlignment="Center">
                            <Ribbon:ButtonPanel Orientation="Horizontal" HorizontalAlignment="Stretch">
                                <Ribbon:ButtonDropDown ImagePosition="Top" Ribbon:RibbonBar.MinAutoSizeHint="Medium" ExpandPosition="Bottom" InlineExpand="False" PopupType="Menu" ContentExpands="False" VerticalContentAlignment="Center" Ribbon:Ribbon.KeyTip="P" Command="{x:Static Commands:InspireCommands.PlayScheduleCommand}">
                                    <Ribbon:ButtonDropDown.Image>
                                        <Image Source="Resources/Images/Play32.png" Stretch="None"/>
                                    </Ribbon:ButtonDropDown.Image>
                                    <Ribbon:ButtonDropDown.Header>Play</Ribbon:ButtonDropDown.Header>
                                </Ribbon:ButtonDropDown>
                            </Ribbon:ButtonPanel>
                        </Ribbon:RibbonBar>
                    </Ribbon:RibbonBarPanel>
                </Ribbon:RibbonTab>
                <!--Designer Home-->
                <Ribbon:RibbonTab Header="Home" x:Name="designerHomeTab" FontSize="10" Ribbon:Ribbon.KeyTip="H">
                    <Ribbon:RibbonBarPanel Height="90" KeyboardNavigation.DirectionalNavigation="Cycle" KeyboardNavigation.TabNavigation="Cycle">
                        <!--Preview-->
                        <Ribbon:RibbonBar VerticalContentAlignment="Center" DialogLauncherVisible="Hidden" Header="Preview" Padding="3" Ribbon:Ribbon.KeyTip="FO">
                            <Ribbon:ButtonPanel Orientation="Horizontal" HorizontalAlignment="Stretch">
                                <Ribbon:ButtonDropDown ImagePosition="Top" Ribbon:RibbonBar.MinAutoSizeHint="Medium" ExpandPosition="Bottom" InlineExpand="False" PopupType="Menu" ContentExpands="False" VerticalContentAlignment="Center" Ribbon:Ribbon.KeyTip="P" Command="{x:Static Commands:InspireCommands.PlayCanvasCommand}">
                                    <Ribbon:ButtonDropDown.Image>
                                        <Image Width="32" Height="32" Source="Resources/Images/PlayPreview32.png"/>
                                    </Ribbon:ButtonDropDown.Image>
                                    <Ribbon:ButtonDropDown.Header>Preview</Ribbon:ButtonDropDown.Header>
                                </Ribbon:ButtonDropDown>
                            </Ribbon:ButtonPanel>
                            <Ribbon:ButtonPanel Orientation="Horizontal" HorizontalAlignment="Stretch">
                                <Ribbon:ButtonDropDown ImagePosition="Top" Ribbon:RibbonBar.MinAutoSizeHint="Medium" ExpandPosition="Bottom" InlineExpand="False" PopupType="Menu" ContentExpands="False" VerticalContentAlignment="Center" Ribbon:Ribbon.KeyTip="S" Command="{x:Static Commands:InspireCommands.StopCanvasCommand}">
                                    <Ribbon:ButtonDropDown.Image>
                                        <Image Width="32" Height="32" Source="Resources/Images/StopPreview32.png"/>
                                    </Ribbon:ButtonDropDown.Image>
                                    <Ribbon:ButtonDropDown.Header>Stop</Ribbon:ButtonDropDown.Header>
                                </Ribbon:ButtonDropDown>
                            </Ribbon:ButtonPanel>
                        </Ribbon:RibbonBar>
                        <!--Clipboard-->
                        <Ribbon:RibbonBar Header="Clipboard" Padding="1" Ribbon:Ribbon.KeyTip="FO" DialogLauncherVisible="Hidden">
                            <Ribbon:ButtonPanel Orientation="Horizontal">
                                <Ribbon:ButtonPanel Margin="2">
                                    <Ribbon:ButtonDropDown ImagePosition="Left" Ribbon:RibbonBar.MinAutoSizeHint="Medium" ExpandPosition="Bottom" InlineExpand="False" PopupType="Menu" ContentExpands="False" VerticalContentAlignment="Center" Ribbon:Ribbon.KeyTip="N" Command="{x:Static Commands:InspireCommands.CutCommand}" >
                                        <Ribbon:ButtonDropDown.Image>
                                            <Image Source="Resources/Images/Cut16.png" Stretch="None"/>
                                        </Ribbon:ButtonDropDown.Image>
                                        <Ribbon:ButtonDropDown.Header>Cut</Ribbon:ButtonDropDown.Header>
                                    </Ribbon:ButtonDropDown>
                                    <Ribbon:ButtonDropDown ImagePosition="Left" Ribbon:RibbonBar.MinAutoSizeHint="Medium" ExpandPosition="Bottom" InlineExpand="False" PopupType="Menu" ContentExpands="False" VerticalContentAlignment="Center" Ribbon:Ribbon.KeyTip="N" Command="{x:Static Commands:InspireCommands.PasteCommand}" >
                                        <Ribbon:ButtonDropDown.Image>
                                            <Image Source="Resources/Images/Paste16.png" Stretch="None"/>
                                        </Ribbon:ButtonDropDown.Image>
                                        <Ribbon:ButtonDropDown.Header>Paste</Ribbon:ButtonDropDown.Header>
                                    </Ribbon:ButtonDropDown>
                                    <Ribbon:ButtonDropDown ImagePosition="Left" Ribbon:RibbonBar.MinAutoSizeHint="Medium" ExpandPosition="Bottom" InlineExpand="False" PopupType="Menu" ContentExpands="False" VerticalContentAlignment="Center" Ribbon:Ribbon.KeyTip="N" Command="{x:Static Commands:InspireCommands.DeleteCommand}" >
                                        <Ribbon:ButtonDropDown.Image>
                                            <Image Width="16" Height="16" Source="Resources/Images/Remove.png"/>
                                        </Ribbon:ButtonDropDown.Image>
                                        <Ribbon:ButtonDropDown.Header>Remove</Ribbon:ButtonDropDown.Header>
                                    </Ribbon:ButtonDropDown>
                                </Ribbon:ButtonPanel>
                            </Ribbon:ButtonPanel>
                            <Ribbon:ButtonPanel>
                                <Ribbon:ButtonPanel Margin="2">
                                    <Ribbon:ButtonDropDown ImagePosition="Top" Ribbon:RibbonBar.MinAutoSizeHint="Medium" ExpandPosition="Bottom" InlineExpand="False" PopupType="Menu" ContentExpands="true" VerticalContentAlignment="Center" Ribbon:Ribbon.KeyTip="C" Command="{x:Static Commands:InspireCommands.CopyCommand}">
                                        <Ribbon:ButtonDropDown.Image>
                                            <Image Source="Resources/Images/Copy32.png" Stretch="None"/>
                                        </Ribbon:ButtonDropDown.Image>
                                        <Ribbon:ButtonDropDown.Header>Copy</Ribbon:ButtonDropDown.Header>
                                        <Ribbon:ButtonDropDown Role="MenuItem" Ribbon:Ribbon.KeyTip="C" Command="{x:Static Commands:InspireCommands.CopyCommand}" Header="Copy"/>
                                        <Ribbon:ButtonDropDown Role="MenuItem" Ribbon:Ribbon.KeyTip="D" Command="{x:Static Commands:InspireCommands.DuplicateCommand}" Header="Duplicate"/>
                                    </Ribbon:ButtonDropDown>
                                </Ribbon:ButtonPanel>
                            </Ribbon:ButtonPanel>
                        </Ribbon:RibbonBar>
                        <!--Slide-->
                        <Ribbon:RibbonBar Header="Slide" Padding="3" Ribbon:Ribbon.KeyTip="FO" DialogLauncherVisible="Hidden">
                            <Ribbon:ButtonPanel Orientation="Horizontal" VerticalAlignment="Stretch">
                                <Ribbon:ButtonDropDown ImagePosition="Top" Ribbon:RibbonBar.MinAutoSizeHint="Medium" ExpandPosition="Bottom" InlineExpand="False" PopupType="Menu" ContentExpands="False" VerticalContentAlignment="Center" Ribbon:Ribbon.KeyTip="N" Command="{x:Static Commands:InspireCommands.NewSlideCommand}">
                                    <Ribbon:ButtonDropDown.Image>
                                        <Image Source="Resources/Images/New32.png" Stretch="None"/>
                                    </Ribbon:ButtonDropDown.Image>
                                    <Ribbon:ButtonDropDown.Header>New</Ribbon:ButtonDropDown.Header>
                                </Ribbon:ButtonDropDown>
                                <Ribbon:ButtonDropDown ImagePosition="Top" Ribbon:RibbonBar.MinAutoSizeHint="Medium" ExpandPosition="Bottom"
            InlineExpand="False" PopupType="Menu" ContentExpands="true" VerticalContentAlignment="Center"
            Ribbon:Ribbon.KeyTip="S" Command="{x:Static Commands:InspireCommands.SaveSlideCommand}"
            ToolTipService.PlacementTarget="{Binding ElementName=AppRibbon}" Header="Save" >
                                    <Ribbon:ButtonDropDown.ToolTip>
                                        <Ribbon:SuperToolTip Header="Save (Ctrl+S)">
                                            <TextBlock TextWrapping="Wrap" Width="180">Save the slide to the server.</TextBlock>
                                        </Ribbon:SuperToolTip>
                                    </Ribbon:ButtonDropDown.ToolTip>
                                    <Ribbon:ButtonDropDown.Image>
                                        <Image Source="Resources/Images/Save32.png" Stretch="None"/>
                                    </Ribbon:ButtonDropDown.Image>
                                    <Ribbon:ButtonDropDown Role="MenuItem" Ribbon:Ribbon.KeyTip="S" Command="{x:Static Commands:InspireCommands.SaveSlideToServerCommand}" Header="Save to Server">
                                        <Ribbon:ButtonDropDown.Image>
                                            <Image Source="Resources/Images/Save16.png" Width="16" Height="16" Stretch="Fill"/>
                                        </Ribbon:ButtonDropDown.Image>
                                    </Ribbon:ButtonDropDown>
                                    <!--<Ribbon:ButtonDropDown Role="MenuItem" Ribbon:Ribbon.KeyTip="L" Command="{x:Static Commands:InspireCommands.SaveSlideLocalCommand}" Header="Save Local (Doesn't upload to server)">
                                        <Ribbon:ButtonDropDown.Image>
                                            <Image Source="Resources/Images/Save16.png" Width="16" Height="16" Stretch="Fill"/>
                                        </Ribbon:ButtonDropDown.Image>
                                    </Ribbon:ButtonDropDown>-->
                                    <Ribbon:ButtonDropDown Role="MenuItem" Ribbon:Ribbon.KeyTip="E" Command="{x:Static Commands:InspireCommands.ExportSlideCommand}" Header="Save Slide to File (Doesn't upload to server)">
                                        <Ribbon:ButtonDropDown.Image>
                                            <Image Source="Resources/Images/New16.png" Stretch="None"/>
                                        </Ribbon:ButtonDropDown.Image>
                                    </Ribbon:ButtonDropDown>
                                </Ribbon:ButtonDropDown>
                                <Ribbon:ButtonDropDown Margin="0,10,0,0" ImagePosition="Top" Ribbon:RibbonBar.MinAutoSizeHint="Medium" ExpandPosition="Bottom" InlineExpand="False" PopupType="Menu" ContentExpands="False" VerticalContentAlignment="Center" Ribbon:Ribbon.KeyTip="N" Command="{x:Static Commands:InspireCommands.ClearSlideCommand}" >
                                    <Ribbon:ButtonDropDown.Image>
                                        <Image Width="20" Height="20" Source="Resources/Images/Remove.png" Stretch="None"/>
                                    </Ribbon:ButtonDropDown.Image>
                                    <Ribbon:ButtonDropDown.Header>Clear</Ribbon:ButtonDropDown.Header>
                                </Ribbon:ButtonDropDown>
                            </Ribbon:ButtonPanel>
                        </Ribbon:RibbonBar>
                        <!--Font-->
                        <Ribbon:RibbonBar Header="Font" Padding="3" DialogLauncherVisible="Hidden" ResizeOrderIndex="2" VerticalContentAlignment="Center" Ribbon:Ribbon.KeyTip="FN">
                            <Ribbon:RibbonBar.CollapsedImage>
                                <Image Source="resources/images/FontColor.png" />
                            </Ribbon:RibbonBar.CollapsedImage>
                            <Ribbon:ButtonPanel Orientation="Horizontal">
                                <Actipro:ButtonGroup Margin="4,0,0,0" HasBorder="False">
                                    <Actipro:FontFamilyComboBox Width="120" IsPreviewEnabled="True" Style="{x:Null}" Command="{x:Static Commands:InspireCommands.FontFamily}" KeyTipAccessText="FF" />
